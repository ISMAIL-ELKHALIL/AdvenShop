{
  "name": "i-shop",
  "version": "1.0.0",
  "description": "my first time learning to build an e-commerce app 2023",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "install-all": "npm install --prefix backend && npm install --prefix frontend/store && npm install --prefix frontend/dashboard",
    "start": "node ./backend/server.js",
    "server": "nodemon backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run store\" \"npm run dashboard\" ",
    "store": "npm start  --prefix frontend/store",
    "dashboard": "npm start --prefix frontend/dashboard",
    "data:import": "node backend/seeder.js -i",
    "data:destroy": "node backend/seeder.js -d"
  },
  "author": "Ismail elkhalil",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.24.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
